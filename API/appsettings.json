{
  "ConnectionStrings": {
    "ServiceBusConnection": "${SB_CONNECTIONSTRING}",
    "AgroDbConnection": "${DB_CONNECTIONSTRING}"
  },
  "CosmosDb": {
    "DatabaseId": "${DATABASE_ID}"
  },
  "Jwt": {
    "Key": "${JWT_KEY}",
    "Issuer": "${JWT_ISSUER}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning",
        "Elastic.Apm": "Warning",
        "Serilog": "Warning"
      }
    }
  },
  "LoggerSettings": {
    "Provider": "${LOGGER_PROVIDER}", // Options: "Database", "NewRelic", "Elastic"
    "ServiceName": "${LOGGER_SERVICE_NAME}"
  },
  "ElasticLogs": {
    "Endpoint": "${ELASTIC_LOGS_ENDPOINT}",
    "ApiKey": "${ELASTIC_LOGS_API_KEY}",
    "IndexPrefix": "${ELASTIC_LOGS_INDEX_PREFIX}"
  },
  "ElasticApm": {
    "Enabled": "${ELASTIC_APM_ENABLED}",
    "ServerUrl": "${ELASTIC_APM_SERVER_URL}",
    "SecretToken": "${ELASTIC_APM_SECRET_TOKEN}",
    "ServiceName": "${APM_SERVICE_NAME}",
    "Environment": "${ENV}"
  },
  "Cors": {
    "AllowedOrigins": []
  },
  "AllowedHosts": "*"
}
